{
	"name":"madeyourday/contao-rocksolid-theme-assistant",
	"description":"Contao Extension: Modify colors and other properties of .css.base and other .base files.",
	"keywords":["contao","themes"],
	"type":"contao-module",
	"homepage":"http://rocksolidthemes.com/de/contao/plugins/theme-assistant",
	"license":"MIT",
	"authors":[
		{
			"name":"RockSolid Themes",
			"homepage":"http://rocksolidthemes.com/de/contao-themes",
			"role":"Developer"
		}
	],
	"support":{
		"forum":"http://help.rocksolidthemes.com/discussions",
		"issues":"https://github.com/madeyourday/contao-rocksolid-theme-assistant/issues",
		"source":"https://github.com/madeyourday/contao-rocksolid-theme-assistant"
	},
	"require":{
		"php":">=5.3"
	},
	"autoload":{
		"psr-4": {
			"MadeYourDay\\RockSolidThemeAssistant\\": "src/"
		},
		"psr-0": {
			"DC_RockSolidThemeAssistant": ""
		}
	},
	"replace":{
		"contao-legacy/rocksolid-theme-assistant":"self.version"
	},
	"extra":{
		"branch-alias": {
			"dev-develop": "2.0.x-dev"
		},
		"contao":{
			"sources":{
				"":"system/modules/rocksolid-theme-assistant"
			}
		}
	}
}
